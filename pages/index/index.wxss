/**index.wxss**/
.borderBottom{
  border-bottom: 1px solid #eeeeee;
}
.container .banner{
  width: 100%;
  display: block;
  height: 468rpx;
}

.container .banner image{
  width: 100%; 
  height: 468rpx;
  background-color: #eeeeee;
  display: block;
} 
.userinfo {
  display: flex;
  align-items: center;
  padding: 0 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 20rpx 0 ;
}
.userinfo.ui-gird-a .ui-block-a,.userinfo.ui-gird-b .ui-block-a{
  font-weight: 200;
  width: 30%;
}
.userinfo.ui-gird-a .ui-block-b{
  width: 70%;
}
.userinfo.ui-gird-b .ui-block-b{
  width: 40%;
}
.userinfo.ui-gird-b .ui-block-c{
  width: 30%;
  text-align: center;
  border-radius:5px;

  line-height: 80rpx;
  box-sizing: border-box;

}


.container form{
  width:100%;
}
.btn-area{
  margin-top: 20rpx;
  padding: 0 20rpx;
}


.container .bottom{
  position: fixed;
  bottom: 10rpx;
}